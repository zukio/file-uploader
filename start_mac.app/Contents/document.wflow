				<key>ActionBundlePath</key>
				<string>/System/Library/Automator/Run AppleScript.action</string>
				<key>ActionName</key>
				<string>AppleScriptを実行</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>-- カレントディレクトリ（f）を基に、APP実行ファイルがあるディレクトリのパスを取得on run {input, parameters}	repeat with i in input		-- 望ましいサブディレクトリへのパスを組み立て		set desiredPath to (i as text) &amp; "dist/mac-universal/"		--display dialog desiredPath -- 変更後のパスを表示		return desiredPath	end repeatend run-- Finder形式のパスをUnix形式に変換するハンドラon convertPathToUnix(finderPath)	-- ドライブ名を"/Volumes/"に置き換え	set volumeName to word 1 of finderPath	set unixPath to "/Volumes/" &amp; text 2 thru -1 of volumeName &amp; finderPath	-- コロン":"をスラッシュ"/"に置き換え	set AppleScript's text item delimiters to ":"	set textItems to text items of unixPath	set AppleScript's text item delimiters to "/"	set unixPath to textItems as string	-- 末尾のスラッシュを除去（もし必要なら）	if unixPath ends with "/" then set unixPath to text 1 thru -2 of unixPath	return unixPathend convertPathToUnix</string>
				</dict>
				<key>ActionRuntimeParameters</key>
				<dict>
					<key>source</key>
					<string>-- カレントディレクトリ（f）を基に、APP実行ファイルがあるディレクトリのパスを取得on run {input, parameters}	repeat with i in input		-- 望ましいサブディレクトリへのパスを組み立て		set desiredPath to (i as text) &amp; "dist/mac-universal/"		--display dialog desiredPath -- 変更後のパスを表示		return desiredPath	end repeatend run-- Finder形式のパスをUnix形式に変換するハンドラon convertPathToUnix(finderPath)	-- ドライブ名を"/Volumes/"に置き換え	set volumeName to word 1 of finderPath	set unixPath to "/Volumes/" &amp; text 2 thru -1 of volumeName &amp; finderPath	-- コロン":"をスラッシュ"/"に置き換え	set AppleScript's text item delimiters to ":"	set textItems to text items of unixPath	set AppleScript's text item delimiters to "/"	set unixPath to textItems as string	-- 末尾のスラッシュを除去（もし必要なら）	if unixPath ends with "/" then set unixPath to text 1 thru -2 of unixPath	return unixPathend convertPathToUnix</string>
